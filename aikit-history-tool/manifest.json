{
  "manifest_version": 2,
  "name": "AIKit History Tool",
  "version": "0.0.1",
  "description": "History management tools for AIKit Orchestrator",
  "permissions": [
    "history"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "aikit-history-tool@bnode.dev",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": ["browsingActivity"],
        "optional": []
      }
    }
  }
}
